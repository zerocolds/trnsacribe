    "start": 457.88,
    "end": 459.2,
    "text": " вы точно до этого дойдете,"
  },
  {
    "start": 459.2,
    "end": 461.68,
    "text": " но просто у вас новый админ,"
  },
  {
    "start": 461.68,
    "end": 462.24,
    "text": " и если вы сейчас,"
  },
  {
    "start": 462.44,
    "end": 464.44,
    "text": " загрузите, они могут..."
  },
  {
    "start": 464.44,
    "end": 467.34000000000003,
    "text": " Да у них и так мозг плавится"
  },
  {
    "start": 467.34000000000003,
    "end": 468.5,
    "text": " от количества информации."
  },
  {
    "start": 468.5,
    "end": 470.18,
    "text": " Да, да, стать бывшими админами."
  },
  {
    "start": 470.18,
    "end": 474.0,
    "text": " Это отчет клиента для обработки."
  },
  {
    "start": 474.0,
    "end": 475.52,
    "text": " Здесь мы сегментируем клиентов,"
  },
  {
    "start": 475.52,
    "end": 476.9,
    "text": " с которыми надо скоммуницировать."
  },
  {
    "start": 476.9,
    "end": 479.34000000000003,
    "text": " Например, там, недошедшие записались"
  },
  {
    "start": 479.34000000000003,
    "end": 481.34,
    "text": " и труханули, подумали,"
  },
  {
    "start": 481.34,
    "end": 482.34,
    "text": " не, не буду прокалывать."
  },
  {
    "start": 482.34,
    "end": 484.56,
    "text": " Админ видит, что клиент там записался,"
  },
  {
    "start": 484.56,
    "end": 485.48,
    "text": " труханул, да, там,"
  },
  {
    "start": 485.48,
    "end": 487.82,
    "text": " недошедших взял, обработал."
  },
  {
    "start": 487.82,
    "end": 489.44,
    "text": " Это я сейчас на демке показываю,"
  },
  {
    "start": 489.44,
    "end": 490.92,
    "text": " у меня есть клиент непрокалыванный."
  },
  {
    "start": 491.76,
    "end": 492.5,
    "text": " Но принцип такой,"
  },
  {
    "start": 492.5,
    "end": 493.74,
    "text": " вот здесь вот клиенты сегментированы."
  },
  {
    "start": 493.74,
    "end": 494.7,
    "text": " Там контроль качества,"
  },
  {
    "start": 494.7,
    "end": 496.34,
    "text": " там все клиенты, которые были там"
  },
  {
    "start": 496.34,
    "end": 497.18,
    "text": " за последние 7 дней,"
  },
  {
    "start": 497.18,
    "end": 498.86,
    "text": " там либо новые, либо повторные,"
  },
  {
    "start": 498.86,
    "end": 499.6,
    "text": " либо и те, и те."
  },
  {
    "start": 499.6,
    "end": 501.36,
    "text": " Обзванивайте, спрашивайте,"
  },
  {
    "start": 501.36,
    "end": 502.8,
    "text": " как ваши лучшики не воспалились,"
  },
  {
    "start": 502.8,
    "end": 505.06,
    "text": " как ощущения, ну, тоже про сервис,"
  },
  {
    "start": 505.06,
    "end": 506.46,
    "text": " про возвращаемость."
  },
  {
    "start": 506.46,
    "end": 507.8,
    "text": " Прогноз клиентов,"
  },
  {
    "start": 507.8,
    "end": 509.92,
    "text": " это когда вы таймер ставите на услугу,"
  },
  {
    "start": 509.92,
    "end": 512.0,
    "text": " говорите, я хочу клиента видеть через 30 дней."
  },
  {
    "start": 512.0,
    "end": 513.84,
    "text": " Отчет говорит, сегодня 30-й день,"
  },
  {
    "start": 513.84,
    "end": 515.0,
    "text": " а клиент еще не записан."
  },
  {
    "start": 515.0,
    "end": 517.1,
    "text": " Ну, и вы здесь дергаете его."
  },
  {
    "start": 517.1,
    "end": 518.42,
    "text": " Приходите там еще."
  },
  {
    "start": 518.42,
    "end": 520.84,
    "text": " Если клиент не вернулся, не записался,"
  },
  {
    "start": 520.84,
    "end": 521.42,
    "text": " он попадет в уходящую,"
  },
  {
    "start": 522.72,
    "end": 524.0,
    "text": " а потом попадет в отток."
  },
  {
    "start": 524.0,
    "end": 528.64,
    "text": " Что нужно сделать?"
  },
  {
    "start": 528.64,
    "end": 531.52,
    "text": " По-хорошему вам бы IP-шку подключить,"
  },
  {
    "start": 531.52,
    "end": 533.58,
    "text": " IP-телефонию, чтобы слушать звонки."
  },
  {
    "start": 533.58,
    "end": 534.6,
    "text": " Есть."
  },
  {
    "start": 534.6,
    "end": 536.32,
    "text": " Молодцы."
  },
  {
    "start": 536.32,
    "end": 537.42,
    "text": " Все, тогда вам больше ничего."
  },
  {
    "start": 537.42,
    "end": 538.84,
    "text": " Все, только слушать,"
  },
  {
    "start": 538.84,
    "end": 540.88,
    "text": " ну, взять за привычку раз в две недели"
  },
  {
    "start": 540.88,
    "end": 542.06,
    "text": " хотя бы с админами устраивать"
  },
  {
    "start": 542.06,
    "end": 543.74,
    "text": " перекрестное прослушивание звонков."
  },
  {
    "start": 543.74,
    "end": 545.68,
    "text": " Как они общаются с клиентом?"
  },
  {
    "start": 545.68,
    "end": 546.54,
    "text": " Представляются?"
  },
  {
    "start": 546.54,
    "end": 548.18,
    "text": " Дает ли клиенту ответить?"
  },
  {
    "start": 548.18,
    "end": 549.48,
    "text": " Не перебивает его?"
  },
  {
    "start": 549.48,
    "end": 551.32,
    "text": " Называют ли там салон,"
  },
  {
    "start": 551.32,
    "end": 551.68,
    "text": " который они..."
  },
  {
    "start": 551.76,
    "end": 552.24,
    "text": " Представляют?"
  },
  {
    "start": 552.24,
    "end": 553.38,
    "text": " Ну, то есть за правила."
  },
  {
    "start": 553.38,
    "end": 554.26,
    "text": " Профилактика."
  },
  {
    "start": 554.26,
    "end": 555.8,
    "text": " Ну, и сами админы, чтобы себя слушали."
  },
  {
    "start": 555.8,
    "end": 558.92,
    "text": " Работали над ошибками."
  },
  {
    "start": 558.92,
    "end": 560.92,
    "text": " И можно будет внедрять работу."
  },
  {
    "start": 560.92,
    "end": 562.1,
    "text": " Здесь вы видите статистику,"
  },
  {
    "start": 562.1,
    "end": 563.62,
    "text": " сколько админ обзанил клиентов,"
  },
  {
    "start": 563.62,
    "end": 565.38,
    "text": " сколько из них записалось там,"
  },
  {
    "start": 565.38,
    "end": 567.2,
    "text": " на какую сумму, на какую сумму дошло."
  },
  {
    "start": 567.2,
    "end": 569.32,
    "text": " Аналитика по звонкам-то,"
  },
  {
    "start": 569.32,
    "end": 570.56,
    "text": " какие ответы дают клиенты."
  },
  {
    "start": 570.56,
    "end": 572.22,
    "text": " В общем, все это вы сможете здесь увидеть."
  },
  {
    "start": 572.22,
    "end": 575.26,
    "text": " С вас контроль и подставленный план."
  },
  {
    "start": 575.26,
    "end": 576.82,
    "text": " Обычно как говорят админу?"
  },
  {
    "start": 576.82,
    "end": 579.16,
    "text": " Админ, ты должен каждый день"
  },
  {
    "start": 579.16,
    "end": 580.68,
    "text": " обрабатывать там 20 клиентов."
  },
  {
    "start": 580.68,
    "end": 581.74,
    "text": " Нет, нет."
  },
  {
    "start": 581.76,
    "end": 584.18,
    "text": " Ты должен делать 20 качественных коммуникаций."
  },
  {
    "start": 584.18,
    "end": 585.88,
    "text": " Нам неинтересно, сколько раз за день"
  },
  {
    "start": 585.88,
    "end": 586.76,
    "text": " админ слушал гудки,"
  },
  {
    "start": 586.76,
    "end": 587.92,
    "text": " сколько раз он не дозвонился."
  },
  {
    "start": 587.92,
    "end": 589.78,
    "text": " Нам интересно именно те коммуникации,"
  },
  {
    "start": 589.78,
    "end": 593.64,
    "text": " где администратор хотя бы представился."
  },
  {
    "start": 593.64,
    "end": 596.04,
    "text": " И мы говорим, идешь слева-направо,"
  },
  {
    "start": 596.04,
    "end": 597.52,
    "text": " обрабатываешь там недошедших,"
  },
  {
    "start": 597.52,
    "end": 598.68,
    "text": " обрабатываешь новых клиентов,"
  },
  {
    "start": 598.68,
    "end": 599.56,
    "text": " которые недавно были,"
  },
  {
    "start": 599.56,
    "end": 601.12,
    "text": " спрашиваешь, как они себя чувствуют."
  },
  {
    "start": 601.12,
    "end": 602.9,
    "text": " Идешь в прогноз визитов,"
  },
  {
    "start": 602.9,
    "end": 604.88,
    "text": " работаешь теми, кто должен прийти,"
  },
  {
    "start": 604.88,
    "end": 605.64,
    "text": " но записи еще нет."
  },
  {
    "start": 605.64,
    "end": 606.86,
    "text": " И потом идешь, рыбачишь,"
  },
  {
    "start": 606.86,
    "end": 607.94,
    "text": " уходящий отток,"
  },
  {
    "start": 607.94,
    "end": 609.06,
    "text": " пытаешься вернуть клиентов."
  },
  {
    "start": 609.06,
    "end": 610.58,
    "text": " Вот такая тема."
  },
  {
    "start": 610.58,
    "end": 611.88,
    "text": " Но я думаю, что вашим админам,"
  },
  {
    "start": 611.88,
    "end": 613.4,
    "text": " если они у вас прям зелененькие, новенькие,"
  },
  {
    "start": 613.4,
    "end": 615.68,
    "text": " ну, рано прям сейчас внедрять."
  },
  {
    "start": 615.68,
    "end": 616.92,
    "text": " Дайте им немножко..."
  },
  {
    "start": 616.92,
    "end": 617.6,
    "text": " На самом деле надо."
  },
  {
    "start": 617.6,
    "end": 618.88,
    "text": " На самом деле надо."
  },
  {
    "start": 618.88,
    "end": 620.04,
    "text": " Они учатся быстро."
  },
  {
    "start": 620.04,
    "end": 622.26,
    "text": " И у них половина дня мы им как раз-таки"
  },
  {
    "start": 622.26,
    "end": 623.64,
    "text": " выделяем на то, чтобы они..."
  },
  {
    "start": 623.64,
    "end": 626.0,
    "text": " Они прямо сейчас уже прописывают в WhatsApp"
  },
  {
    "start": 626.0,
    "end": 629.02,
    "text": " людей, которые три месяца назад были."
  },
  {
    "start": 629.02,
    "end": 630.58,
    "text": " Просто вот слепую заходят,"
  },
  {
    "start": 630.58,
    "end": 632.88,
    "text": " смотрят, что, где и как."
  },
  {
    "start": 632.88,
    "end": 634.76,
    "text": " Что их там не было пять месяцев,"
  },
  {
    "start": 634.76,
    "end": 635.58,
    "text": " четыре-три месяца."
  },
  {
    "start": 635.58,
    "end": 637.54,
    "text": " Вот, и прописывают."
  },
  {
    "start": 637.54,
    "end": 639.7,
    "text": " Ага, писать хорошо."
  },
  {
    "start": 639.7,
    "end": 639.8,
    "text": " Хорошо."
  },
  {
    "start": 640.16,
    "end": 641.24,
    "text": " Ну, и вот все сообщения"
  },
  {
    "start": 641.24,
    "end": 642.92,
    "text": " лучше, конечно, на чат-бот сбросить."
  },
  {
    "start": 642.92,
    "end": 644.76,
    "text": " Автоматизировать."
  },
  {
    "start": 644.76,
    "end": 645.84,
    "text": " Чтобы администратор..."
  },
  {
    "start": 645.84,
    "end": 647.78,
    "text": " У нас автоматизировано,"
  },
  {
    "start": 647.78,
    "end": 650.56,
    "text": " но еще дополнительно админов подрубали,"
  },
  {
    "start": 650.56,
    "end": 651.24,
    "text": " чтобы..."
  },
  {
    "start": 651.24,
    "end": 652.94,
    "text": " Лучше, чтобы админы звонили, правильно?"
  },
  {
    "start": 652.94,
    "end": 653.96,
    "text": " Да, да."
  },
  {
    "start": 653.96,
    "end": 655.28,
    "text": " Да, у звонков больше конверсия."
  },
  {
    "start": 655.28,
    "end": 656.34,
    "text": " По оттоку, например,"
  },
  {
    "start": 656.34,
    "end": 658.88,
    "text": " у клиентов, у которых там не были больше ста дней,"
  },
  {
    "start": 658.88,
    "end": 660.96,
    "text": " у сообщений конверсия в районе пяти процентов,"
  },
  {
    "start": 660.96,
    "end": 662.28,
    "text": " а у звонков в районе пятнадцати."
  },
  {
    "start": 662.28,
    "end": 664.68,
    "text": " А если это админ с поставленной речью,"
  },
  {
    "start": 664.68,
    "end": 666.32,
    "text": " то может и всех двадцать процентов."
  },
  {
    "start": 666.32,
    "end": 668.4,
    "text": " Там, здрасте, тут ваш мастер Даниил,"
  },
  {
    "start": 668.4,
    "end": 669.5,
    "text": " интересуется, как у вас дела."
  },
  {
    "start": 669.74,
    "end": 672.06,
    "text": " Просил вам набрать, как самочувствие,"
  },
  {
    "start": 672.06,
    "end": 674.2,
    "text": " как ушки поживают, приходите на обновку."
  },
  {
    "start": 674.2,
    "end": 675.64,
    "text": " В общем, ждал на чай."
  },
  {
    "start": 675.64,
    "end": 678.48,
    "text": " Это же совершенно другое, чем сообщение какое-то бездушное, правильно?"
  },
  {
    "start": 678.48,
    "end": 682.44,
    "text": " Но вот к этому надо подойти ответственно."
  },
  {
    "start": 682.44,
    "end": 684.36,
    "text": " Я просто не верю, что ваши админы сейчас..."
  },
  {
    "start": 684.36,
    "end": 686.48,
    "text": " Две недели, вы сами говорите, работают."
  },
  {
    "start": 686.48,
    "end": 688.54,
    "text": " Они у вас там сейчас запариваются."
  },
  {
    "start": 688.54,
    "end": 690.54,
    "text": " Ну да, они так охеревают."
  },
  {
    "start": 690.54,
    "end": 693.1,
    "text": " Они еще не освоились, они там в ахере сидят."
  },
  {
    "start": 693.1,
    "end": 695.1,
    "text": " Вы такие, так, ребята, еще..."
  },
  {
    "start": 695.1,
    "end": 697.26,
    "text": " Да, вы ему сейчас такие, еще звонить надо."
  },
  {
    "start": 697.26,
    "end": 699.22,
    "text": " Они такие, нет, пожалуй, пойду поищу,"
  },
  {
    "start": 699.32,
    "end": 699.92,
    "text": " другое место."
  },
  {
    "start": 699.92,
    "end": 703.94,
    "text": " Не передавить бы, потихонечку, потихонечку."
  },
  {
    "start": 703.94,
    "end": 705.4,
    "text": " Может быть, еще через две недельки"
  },
  {
    "start": 705.4,
    "end": 707.1,
    "text": " можно будет уже звонить,"
  },
  {
    "start": 707.1,
    "end": 708.26,
    "text": " начинать потихонечку."
  },
  {
    "start": 708.26,
    "end": 710.62,
    "text": " Но прям, ну,"
  },
  {
    "start": 710.62,
    "end": 712.52,
    "text": " с наскока я бы не стал."
  },
  {
    "start": 712.52,
    "end": 715.12,
    "text": " Пускай они сначала разберутся,"
  },
  {
    "start": 715.12,
    "end": 717.54,
    "text": " как там, настройте оперативные записи,"
  },
  {
    "start": 717.54,
    "end": 719.44,
    "text": " чтобы они записывали клиента сразу на следующий день."
  },
  {
    "start": 719.44,
    "end": 721.32,
    "text": " Какие-нибудь основы сервиса,"
  },
  {
    "start": 721.32,
    "end": 723.26,
    "text": " встречи гостей пока что проработаете,"
  },
  {
    "start": 723.26,
    "end": 724.82,
    "text": " как они там встречают, провожают,"
  },
  {
    "start": 724.82,
    "end": 725.5,
    "text": " что они говорят."
  },
  {
    "start": 725.5,
    "end": 728.9,
    "text": " И уже потом можно будет..."
  },
  {
    "start": 728.9,
    "end": 730.26,
    "text": " Звонки подключать."
  },
  {
    "start": 730.26,
    "end": 732.16,
    "text": " Пока можете начать, ребята, с аналитики."
  },
  {
    "start": 732.16,
    "end": 734.22,
    "text": " Ну да."
  },
  {
    "start": 734.22,
    "end": 737.98,
    "text": " Окей, как-то все работает."
  },
  {
    "start": 737.98,
    "end": 740.04,
    "text": " Вы на всю эту тему подписывайтесь."
  },
  {
    "start": 740.04,
    "end": 741.76,
    "text": " Ну, как вот на музыку подписывайтесь,"
  },
  {
    "start": 741.76,
    "end": 743.32,
    "text": " также подписывайтесь на аналитику."
  },
  {
    "start": 743.32,
    "end": 747.7,
    "text": " У нас есть тарифы по 1, 2, 3, 6 и 12 месяцев."
  },
  {
    "start": 747.7,
    "end": 749.52,
    "text": " Сейчас я вам дайте на экран все выделить."
  },
  {
    "start": 749.52,
    "end": 752.08,
    "text": " Вам нужно определиться,"
  },
  {
    "start": 752.08,
    "end": 753.2,
    "text": " какие отчеты вам нужны."
  },
  {
    "start": 753.2,
    "end": 753.8,
    "text": " У нас два отчета."
  },
  {
    "start": 753.8,
    "end": 754.88,
    "text": " Два отчета я вам показал."
  },
  {
    "start": 754.88,
    "end": 756.62,
    "text": " KPI, это аналитика."
  },
  {
    "start": 756.62,
    "end": 758.86,
    "text": " И КДО, клиента для обработки."
  },
  {
    "start": 758.9,
    "end": 760.24,
    "text": " Я вам такой же калькуляторик пришлю."
  },
  {
    "start": 760.24,
    "end": 760.8,
    "text": " Поиграйтесь."
  },
  {
    "start": 760.8,
    "end": 763.42,
    "text": " Поставите там, что нужно, вкл-вкл."
  },
  {
    "start": 763.42,
    "end": 765.8,
    "text": " И смотрите."
  },
  {
    "start": 765.8,
    "end": 768.9,
    "text": " 1, 3, 6 и 12 месяцев."
  },
  {
    "start": 768.9,
    "end": 770.96,
    "text": " Чем пакет больше, тем он выгонит."
  },
  {
    "start": 770.96,
    "end": 772.2,
    "text": " Там уже по бюджету выбирайте."
  },
  {
    "start": 772.2,
    "end": 773.94,
    "text": " Грубо говоря, все вместе."
  },
  {
    "start": 773.94,
    "end": 775.32,
    "text": " Один месяц 5, 3, 35."
  },
  {
    "start": 775.32,
    "end": 778.38,
    "text": " В подписку уходит обучение."
  },
  {
    "start": 778.38,
    "end": 780.6,
    "text": " В следующий раз в демонстрацию будете включать вы."
  },
  {
    "start": 780.6,
    "end": 782.42,
    "text": " А я вам буду показывать, куда кликать,"
  },
  {
    "start": 782.42,
    "end": 783.92,
    "text": " куда заходить, как графики читать."
  },
  {
    "start": 783.92,
    "end": 787.58,
    "text": " Ну и поддержка, если что-то забыли."
  },
  {
    "start": 787.58,
    "end": 788.64,
    "text": " Ребята, как считается,"
  },
  {
    "start": 788.74,
    "end": 789.3,
    "text": " возвращаемость?"
  },
  {
    "start": 789.3,
    "end": 791.94,
    "text": " Пишите нам в чате, и коллеги мои отвечают."
  },
  {
    "start": 791.94,
    "end": 794.76,
    "text": " Возвращаем, считается так-то, так-то."
  },
  {
    "start": 794.76,
    "end": 796.62,
    "text": " Посмотрите, вы можете там-то, там-то, так-то."
  },
  {
    "start": 796.62,
    "end": 799.22,
    "text": " Это все входит в подписку."
  },
  {
    "start": 799.22,
    "end": 802.9,
    "text": " С вас только усердно заниматься"
  },
  {
    "start": 802.9,
    "end": 804.62,
    "text": " с отчетами."
  },
  {
    "start": 804.62,
    "end": 809.4,
    "text": " Как-то так."
  },
  {
    "start": 809.4,
    "end": 811.72,
    "text": " Понятно."
  },
  {
    "start": 811.72,
    "end": 815.64,
    "text": " И скажите, кто именно сдал нас,"
  },
  {
    "start": 815.64,
    "end": 817.86,
    "text": " от них к любому должен быть"
  },
  {
    "start": 817.86,
    "end": 818.72,
    "text": " какой-то промокодик,"
  },
  {
    "start": 818.74,
    "end": 819.74,
    "text": " я поищу."
  },
  {
    "start": 819.74,
    "end": 822.0,
    "text": " 10% скидка на первый пакет"
  },
  {
    "start": 822.0,
    "end": 823.22,
    "text": " от друзей."
  },
  {
    "start": 823.22,
    "end": 824.46,
    "text": " У нас рефералка работает."
  },
  {
    "start": 824.46,
    "end": 828.46,
    "text": " Слушай, они одновременно практически"
  },
  {
    "start": 828.46,
    "end": 829.16,
    "text": " у меня скидали."
  },
  {
    "start": 829.16,
    "end": 831.6,
    "text": " Это Катман и Фармин."
  },
  {
    "start": 831.6,
    "end": 835.02,
    "text": " Муза и Рома, я понял."
  },
  {
    "start": 835.02,
    "end": 836.22,
    "text": " Кого вы больше любите?"
  },
  {
    "start": 836.22,
    "end": 837.12,
    "text": " Да, Рома."
  },
  {
    "start": 837.12,
    "end": 838.76,
    "text": " Давай Ромочки."
  },
  {
    "start": 838.76,
    "end": 841.1,
    "text": " Окей."
  },
  {
    "start": 841.1,
    "end": 842.04,
    "text": " Все."
  },
  {
    "start": 842.04,
    "end": 844.66,
    "text": " Потому что им тоже 10% скидки на следующий пакет."
  },
  {
    "start": 844.66,
    "end": 848.54,
    "text": " Все, поставлю."
  },
  {
    "start": 848.74,
    "end": 850.28,
    "text": " В общем, выбирайте, ребята."
  },
  {
    "start": 850.28,
    "end": 851.58,
    "text": " Определяйтесь пакетом подписки"
  },
  {
    "start": 851.58,
    "end": 852.56,
    "text": " и прямо в чате пишите."
  },
  {
    "start": 852.56,
    "end": 854.7,
    "text": " Друзья, мы хотим вот это, вот это,"
  },
  {
    "start": 854.7,
    "end": 855.44,
    "text": " вот настолько-то."
  },
  {
    "start": 855.44,
    "end": 857.12,
    "text": " Коллеги попросят у вас реквизиты."
  },
  {
    "start": 857.12,
    "end": 859.42,
    "text": " Вы отправляете реквизиты."
  },
  {
    "start": 859.42,
    "end": 860.8,
    "text": " Они вам оставляют счетик."
  },
  {
    "start": 860.8,
    "end": 862.5,
    "text": " Счетик оплачиваете,"
  },
  {
    "start": 862.5,
    "end": 863.38,
    "text": " и мы на следующей недельке"
  },
  {
    "start": 863.38,
    "end": 864.58,
    "text": " договариваемся с вами на обучение."
  },
  {
    "start": 864.58,
    "end": 866.16,
    "text": " Хорошо."
  },
  {
    "start": 866.16,
    "end": 868.38,
    "text": " Все."
  },
  {
    "start": 868.38,
    "end": 868.92,
    "text": " Хорошо."
  },
  {
    "start": 868.92,
    "end": 871.04,
    "text": " Тогда вам всем желаю хорошего дня,"
  },
  {
    "start": 871.04,
    "end": 873.02,
    "text": " хороших ходных, если они у вас будут."
  },
  {
    "start": 873.02,
    "end": 874.38,
    "text": " И хорошо."
  },
  {
    "start": 874.38,
    "end": 875.4,
    "text": " Не в этой жизни."
  },
  {
    "start": 875.4,
    "end": 878.5,
    "text": " Выходные для лохов."
  },
  {
    "start": 878.74,
    "end": 879.22,
    "text": " Отдыхаем."
  },
  {
    "start": 879.22,
    "end": 881.38,
    "text": " Я понял, я понял."
  },
  {
    "start": 881.38,
    "end": 882.94,
    "text": " Ну, хорошо, ребят."
  },
  {
    "start": 882.94,
    "end": 883.48,
    "text": " Все."
  },
  {
    "start": 883.48,
    "end": 884.46,
    "text": " Тогда будем на связи."
  },
  {
    "start": 884.46,
    "end": 885.32,
    "text": " Пока-пока."
  },
  {
    "start": 885.32,
    "end": 886.34,
    "text": " Хорошего дня."
  }
]